{
  "extends": ["react-app", "airbnb", "eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "off",
    "max-len": [
      "error",
      {
        "code": 140,
        "ignoreStrings": true
      }
    ],
    "linebreak-style": 0
  }
}
